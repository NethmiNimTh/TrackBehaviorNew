	"item": [
		{
			"name": "1. Health Check",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/api/health",
					"host": ["{{base_url}}"],
					"path": ["api", "health"]
				},
				"description": "Check server health and database connectivity"
			}
		},
		{
			"name": "2. Register User",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 201) {",
							"    var jsonData = pm.response.json();",
							"    pm.environment.set(\"auth_token\", jsonData.token);",
							"    pm.environment.set(\"user_email\", jsonData.user.email);",
							"    console.log(\"Token saved: \" + jsonData.token);",
							"}"
						]
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"email\": \"test@demo.com\",\n    \"password\": \"test123\"\n}"
				},
				"url": {
					"raw": "{{base_url}}/api/register",
					"host": ["{{base_url}}"],
					"path": ["api", "register"]
				},
				"description": "Register a new user and get authentication token"
			}
		},
		{
			"name": "3. Login User",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 200) {",
							"    var jsonData = pm.response.json();",
							"    pm.environment.set(\"auth_token\", jsonData.token);",
							"    pm.environment.set(\"device_id\", jsonData.device_id);",
							"    console.log(\"Token saved: \" + jsonData.token);",
							"    console.log(\"Device ID: \" + jsonData.device_id);",
							"}"
						]
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"email\": \"test@demo.com\",\n    \"password\": \"test123\"\n}"
